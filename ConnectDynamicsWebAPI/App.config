<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.2" />
    </startup>
  <appSettings>
    <!--Azure AD Directory ID-->
    <add key="TenantId" value="" />
    
    <add key="TokenUrl" value="https://login.microsoftonline.com/@TenantId/oauth2/token" />
    <add key="GrantType" value="client_credentials" />
    
    <!--Application Id of Azure registered app-->
    <add key="ClientId" value="" /> 
    
    <!--Applicatio secret of Azure registered app-->
    <add key="ClientSecret" value="" />

    <!--Dynamics URL (https://....crm6.dynamics.com)-->
    <add key="ResourceUrl" value="" />
  </appSettings>
</configuration>